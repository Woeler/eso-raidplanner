{
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Woeler",
            "email": "woeler@woeler.eu",
            "role": "Developer",
            "homepage": "https://woeler.dev"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-cache-bundle": "^1.4",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "easycorp/easyadmin-bundle": "^2.3",
        "eluceo/ical": "^0.16.0",
        "erusev/parsedown": "^1.7",
        "knplabs/knp-menu-bundle": "^2.2",
        "knplabs/knp-paginator-bundle": "^5.2",
        "knpuniversity/oauth2-client-bundle": "^1.28",
        "sensio/framework-extra-bundle": "^5.1",
        "simshaun/recurr": "^4.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/lock": "4.4.*",
        "symfony/messenger": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "4.4.*",
        "tattali/calendar-bundle": "^1.1",
        "tetranz/select2entity-bundle": "^3.1",
        "woeler/phpdiscord": "^1.1",
        "wohali/oauth2-discord-new": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "bin-dir": "bin/",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cgl": [
            "mkdir -p var/php-cs-fixer/; php-cs-fixer fix --config build/.php_cs --format=junit > var/php-cs-fixer/php-cs-fixer.xml"
        ],
        "make:env": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
