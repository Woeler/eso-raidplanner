{% macro linkbutton(link, label, class = 'default', icon = null, fab = false) %}
    <a href="{{ link }}" class="btn btn-{{ class }}">
        {% if icon %}<span class="btn-icon"><i class="fa{{ fab ? 'b' : '' }} fa-{{ icon }}"></i></span>{% endif %}
        <span class="btn-text">{{ label }}</span>
    </a>
{% endmacro %}